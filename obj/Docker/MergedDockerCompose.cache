services:
  calculodejuros.api:
    build:
      context: C:\Users\Usuario\source\repos\JurosSolution
      dockerfile: CalculoDeJuros.API\Dockerfile
    depends_on:
    - taxadejuros.api
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://localhost:80
      TaxaDeJurosAPI: http://taxadejuros.api/
    image: calculodejurosapi
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Usuario\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\Usuario\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
  taxadejuros.api:
    build:
      context: C:\Users\Usuario\source\repos\JurosSolution
      dockerfile: TaxaDeJuros.API\Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://localhost:80
    image: taxadejurosapi
    ports:
    - published: 5105
      target: 80
    volumes:
    - C:\Users\Usuario\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\Usuario\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
version: '3.4'